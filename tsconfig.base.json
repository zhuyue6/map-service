{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "jsx": "preserve",
    "jsxFactory": "React.createElement",
    "esModuleInterop": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@web-map-service/*": ["./packages/*/src"]
    }
  },
  "exclude": [
    "node_modules"
  ]
}